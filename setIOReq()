public int setIOReq() {
		double m = Math.random();
		int n = 0;
		if (m >= 0 && m < 0.2) {
			n = 0;
		} else if (m >= 0.2 && m < 0.4) {
			n = 1;
		} else if (m >= 0.4 && m < 0.6) {
			n = 2;
		} else if (m >= 0.6 && m < 0.8) {
			n = 3;
		} else if (m >= 0.8 && m < 0.9) {
			n = 4;
		} else if (m >= 0.9 && m <= 1) {
			n = 5;
		}
		return n;
	}
